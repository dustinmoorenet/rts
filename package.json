{
  "name": "rts",
  "version": "1.0.0",
  "description": "A real time strategy game that will never see the light of day",
  "repository": "https://github.com/dustinmoorenet/rts",
  "main": "index.js",
  "scripts": {
    "test": "NODE_PATH=./src mocha src/js/tests/**/test.*.js --reporter spec --compilers js:babel-register",
    "build:css": "node-sass ./src/sass/index.scss ./dist/app.css",
    "watch:css": "npm run build:css && node-sass --watch --source-map=true ./src/sass/index.scss ./dist/app.css",
    "build:js": "webpack -d",
    "watch:js": "webpack -d --watch",
    "watch:html": "watch 'npm run build:html' src/html",
    "build:html": "mkdir -p dist && cp ./src/html/* dist/.",
    "build": "npm run build:html && npm run build:css && npm run build:js",
    "watch": "npm run watch:html & npm run watch:css & npm run watch:js",
    "start": "node scripts/server.js & npm run watch"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-class-properties": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-plugin-transform-runtime": "6.8.0",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.8.0",
    "babel-runtime": "6.6.1",
    "classnames": "2.2.5",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "9.0.0",
    "eslint-plugin-import": "1.7.0",
    "eslint-plugin-jsx-a11y": "1.2.0",
    "eslint-plugin-react": "5.0.1",
    "events-mixin": "1.3.0",
    "expect": "1.20.1",
    "express": "4.13.4",
    "jsdom": "9.0.0",
    "lodash": "4.11.2",
    "mocha": "2.4.5",
    "node-sass": "3.7.0",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "reduce-reducers": "0.1.2",
    "redux": "3.5.2",
    "redux-thunk": "2.0.1",
    "reselect": "2.5.1",
    "watch": "0.18.0",
    "webpack": "1.13.0"
  },
  "author": "Dustin Moore",
  "license": "ISC"
}
